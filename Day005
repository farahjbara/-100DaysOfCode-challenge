# In[1]:


def counter(start, stop):
	x = start
	if x>stop:
		return_string = "Counting down: "
		while x >= stop:
			return_string += str(x)
			if x>stop:
				return_string += ","
			x = x-1
	else:
		return_string = "Counting up: "
		while x <= stop:
			return_string += str(x)
			if x<stop:
				return_string += ","
			x = x+1
	return return_string

print(counter(1, 10)) 
print(counter(2, 1)) 
print(counter(5, 5)) 


# In[5]:


def votes(params):
	for vote in params:
	    print("Possible option:" + vote)


